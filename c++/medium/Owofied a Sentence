//https://edabit.com/challenge/G3hRLmmrcMaGAbf6F
std::string owofied(std::string sentence) {
	std::string stringer;
	for(int i = 0; i<sentence.length(); i++){
		if(sentence.at(i) == 'i'){
			stringer.push_back('w');
			stringer.push_back('i');
		}
		
		if (sentence.at(i) == 'e') {
			stringer.push_back('w');
			stringer.push_back('e');
		}
		stringer.push_back(i);
		stringer.append(" owo");
	}
	std::cout << stringer;
}
