std::string preventDistractions(std::string str) {
	std::string AniWord = "anime";
	std::string MemWord = "meme";
	std::string DeadPlatform = "vines";
	std::string RoWord = "roasts";
	std::string DeVito = "Danny DeVito";
	size_t found = str.find(AniWord);
	size_t found1 = str.find(MemWord);
	size_t found2 = str.find(DeadPlatform);
	size_t found3 = str.find(RoWord);
	size_t found4 = str.find(DeVito);
	if(found!=std::string::npos){
		return "NO!";
	}
	else if(found1!=std::string::npos){
		return "NO!";
	}
	else if(found2!=std::string::npos){
		return "NO!";
	}
	else if(found3!=std::string::npos){
		return "NO!";
	}
	else if(found4!=std::string::npos){
		return "NO!";
	}
	else
		return "Safe watching!";
}


